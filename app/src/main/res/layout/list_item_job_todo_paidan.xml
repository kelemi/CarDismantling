<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
     android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/padding10"
    android:background="@color/white"
    >
    <TextView
        android:id="@+id/tv_chaijie_renwu_state"
        style="@style/NormalStyle"
        android:text="任务状态"
        android:textColor="@color/color_main"
        android:textSize="@dimen/word16"/>
    <TextView
        android:id="@+id/tv_car_num"
        style="@style/NormalStyle"
        android:layout_below="@+id/tv_chaijie_renwu_state"
        android:layout_marginTop="@dimen/margin10"
        android:text="车牌号"
        android:textColor="@color/gray"
        android:textSize="@dimen/word16"/>
    <TextView
        android:id="@+id/tv_chaijie_worker"
        style="@style/NormalStyle"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/tv_chaijie_renwu_state"
        android:layout_marginTop="@dimen/margin10"
        android:text="拆解人员:xxxxxx"
        android:textColor="@color/gray"
        android:textSize="@dimen/word16"/>
    <TextView
        android:id="@+id/tv_chaijie_type"
        style="@style/WidthFullStyle"
        android:layout_below="@+id/tv_car_num"
        android:layout_marginTop="@dimen/margin10"
        android:text="拆解方式"
        android:textColor="@color/red2"
        android:textSize="@dimen/word16"
        android:singleLine="true"
        android:ellipsize="end"
        android:visibility="gone"/>
    <TextView
        android:id="@+id/tv_chaijie_lingjian"
        style="@style/WidthFullStyle"
        android:layout_below="@+id/tv_chaijie_type"
        android:layout_marginTop="@dimen/margin10"
        android:text="拆解零件:xxxxxx"
        android:textColor="@color/gray"
        android:textSize="@dimen/word16"
        android:singleLine="true"
        android:ellipsize="end"/>
</RelativeLayout>