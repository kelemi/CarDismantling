<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/grayqing">
    <include
        layout="@layout/common_title_bar"/>
    <TextView
        android:id="@+id/tv_car_kucun"
        style="@style/WidthFullStyle"
        android:background="@drawable/selector_back_white_gray"
        android:drawableRight="@drawable/ico_back_right_black"
        android:padding="@dimen/padding10"
        android:gravity="center_vertical"
        android:text="汽车库存"
        android:textSize="@dimen/word16"/>
    <TextView
        android:id="@+id/tv_lingjian_kucun"
        style="@style/WidthFullStyle"
        android:layout_marginTop="@dimen/margin1"
        android:background="@drawable/selector_back_white_gray"
        android:drawableRight="@drawable/ico_back_right_black"
        android:padding="@dimen/padding10"
        android:gravity="center_vertical"
        android:text="零件库存"
        android:textSize="@dimen/word16"/>
</LinearLayout>